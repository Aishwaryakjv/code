class Main {
  public static void main(String[] args) {
    int arr[]={0,1,1,2,3};
    int n=5,count=0;
    int k=0;
    int ch[]=new int[10];
    for(int i=0;i<9;i++)
      {
        for(int j=0;j<n;j++)
          {
            if(i==arr[j])
            {
              count++;
            }  
          }
        ch[k++]=count;
        count=0;
      }
    for(int i=0;i<10;i++)
    System.out.println(ch[i]+" ");
  }
}
